<table>
    <thead>
    <tr>
        <th>title</th>
        <th> release year</th>
        <th> artist name </th>
        <th>genre</th>
    </tr>
    </thead>
    <tbody>
    <% @songs.each do |song| %>
        <tr>
            <td><%= song.title  %></td>
            <td><%= song.release_year  %></td>
            <td><%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
    <% end %>
    </tbody
</table>